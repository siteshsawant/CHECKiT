{
  "manifest_version": 2,
  "name": "CHECKiT",
  "version": "1.0",
  "content_scripts": [
  {
		"matches": [
			"<all_urls>"
		],
		"js":["content.js"]
  }
  ],
  "background":{
    "scripts":["background.js"],
    "persistent":false
},
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "description": "",
  "permissions": [
  "activeTab",
      "contextMenus",
      "storage",
  "tabs"
  ],
   "icons": {    
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
       },    
   "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  }

}



























































